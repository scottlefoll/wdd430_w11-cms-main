{
  "version": 2,
  "routes": [
    {
      "src": "/api/messages(.*)",
      "dest": "/api/messages$1"
    },
    {
      "src": "/api/contacts(.*)",
      "dest": "/api/contacts$1"
    },
    {
      "src": "/api/documents(.*)",
      "dest": "/api/documents$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/cms/browser/index.html"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ]
}
