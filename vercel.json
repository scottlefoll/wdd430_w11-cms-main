{
  "version": 2,
  "builds": [
    {
      "src": "src/app",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs12.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/cms/browser/index.html"
    }
  ]
}
